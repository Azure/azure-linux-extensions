[
  {
    "name":  "OmsAgentForLinux",
    "version": "1.13.5",
    "handlerManifest": {
      "installCommand": "omsagent_shim.sh -install",
      "uninstallCommand": "omsagent_shim.sh -uninstall",
      "updateCommand": "omsagent_shim.sh -update",
      "enableCommand": "omsagent_shim.sh -enable",
      "disableCommand": "omsagent_shim.sh -disable",
      "rebootAfterInstall": false,
      "reportHeartbeat": false,
      "updateMode": "UpdateWithInstall",
      "stopOnMultipleConnections": "true",
      "continueOnUpdateFailure": "true"
    }
  }
]
